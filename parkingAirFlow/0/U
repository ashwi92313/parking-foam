FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            pressureInletVelocity;      //other option fixed value of 0
    }
    outlet
    {
        
        type            flowRateOutletVelocity;
        value           uniform (98.17 0 0); //for v=5 m/s d=5m //other option pressureInletOutletVelocity
    }
    walls
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
