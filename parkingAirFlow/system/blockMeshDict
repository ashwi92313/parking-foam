vertices        
(
    (0 0 0)
    (50 0 0)
    (50 10 0)
    (0 10 0)
    (0 0 0.01)
    (50 0 0.01)
    (50 10 0.01)
    (0 10 0.01)
    (0 15 0)
    (50 15 0)
    (0 15 .01)
    (50 15 .01)
    (0 20 0)
    (50 20 0)
    (0 20 .01)
    (50 20 .01)
    (0 30 0)
    (50 30 0)
    (0 30 .01)
    (50 30 .01)
);

blocks          
(
    hex (0 1 2 3 4 5 6 7) (500 100 1) simpleGrading (1 1 1)
    hex (3 2 9 8 7 6 11 10) (500 50 1) simpleGrading (1 1 1)
);

boundary         
(
    walls
    {
        type    wall;
        faces
        (
            (0 4 7 3)
            (2 6 5 1)
            (1 5 4 0)
            (3 7 10 8) // 2 left
            (8 10 11 9) // 2 top
        );
