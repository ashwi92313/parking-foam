            (9 11 6 2) // 2 right
        );
    }
    frontAndBack 
    {
        type    empty;
        faces
        (
            (0 3 2 1)
            (4 5 6 7)
            (3 8 9 2) // 2
            (7 6 11 10) // 2
        );
    }
