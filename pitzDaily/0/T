FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0]; // mol/m^3

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            zeroGradient;
    }
    leftWall
    {
        type            fixedValue;
	value		uniform    0;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    rightWall
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

